          "name": "order_items_test",
          "test_data": {
            "OrderItems": [
              {
                "SellerSKU": "SKU-001",
                "QuantityOrdered": 2,
                "ItemPrice": {
                  "Amount": "39.99",
                  "CurrencyCode": "GBP"
                },
                "Title": "Example Product"
              }
            ]
          },
          "expected_result": [
            {
              "sku": "SKU-001",
              "quantity": 2,
              "price": "39.99",
              "title": "Example Product"
            }
          ]
        }
      ]
    },
